<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    {% if page.full_title %}
    <title>{{ page.full_title|raw }}</title>
    {% else %}
    <title>{{ page.title|raw }} &mdash; {{ site.title|raw }}</title>
    {% endif %}
    <meta name="description" content="{{ site.meta.description }}">
    <meta name="author" content="{{ site.meta.author.name }}">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel="stylesheet" href="{{site.url}}/legacy/css/boilerplate.css">
    <link rel="stylesheet" href="{{site.url}}/legacy/css/style.css">
    <link rel="stylesheet" href="{{site.url}}/legacy/css/helpers.css">
    <link rel="shortcut icon" href="{{ site.url }}/favicon.ico" />
    <script src="{{site.url}}/legacy/js/libs/modernizr-2.0.6.min.js"></script>
    <meta property="og:locale" content="en_US" />
    {% if page.og_title %}<meta property="og:title" content="{{ page.og_title|raw }}"/>
    {% else %}<meta property="og:title" content="{{ page.title|raw }} &mdash; {{ site.title|raw }}" />
    {% endif %}
    {% if page.og_url %}<meta property="og:url" content="{{ page.og_url|raw }}"/>
    {% endif %}
    {% if page.og_description %}<meta property="og:description" content="{{ page.og_description|raw }}"/>
    {% endif %}
    {% if page.og_type %}<meta property="og:type" content="{{ page.og_type|raw }}"/>
    {% endif %}
    {% if page.og_image %}<meta property="og:image" content="{{site.url}}{{page.og_image}}"/>
    {% endif %}
    {% if page.og_image_width %}<meta property="og:image:width" content="{{page.og_image_width}}"/>
    {% endif %}
    {% if page.og_image_height %}<meta property="og:image:height" content="{{page.og_image_height}}"/>
    {% endif %}
    {% if page.og_image_type %}<meta property="og:image:type" content="{{page.og_image_type}}"/>
    {% endif %}
</head>
<body>
    {% if not page.no_offloc_primary_navigation %}
    <nav id="offloc-primary-navigation"><a class="offloc-icon" href="http://offloc.com">Offloc</a></nav>
    {% endif %}
    {% block content %}
    {{ content }}
    {% endblock content %}
    <footer class="site">
        {% if page.no_offloc_primary_navigation %}
        <a class="offloc-icon" href="{{site.url}}/">Offloc</a><br />
        {% endif %}
        &copy;Offloc Incorporated, 2010-2012<br />
        All Rights Reserved
    </footer>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{site.url}}/legacy/js/libs/jquery-1.6.4.min.js"><\/script>')</script>
    <script defer src="{{site.url}}/legacy/js/plugins.js"></script>
    <script defer src="{{site.url}}/legacy/js/script.js"></script>
    <script type="text/javascript">

      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-18722842-1']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();

    </script>
</body>
</html>
